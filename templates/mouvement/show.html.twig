{% extends 'base.html.twig' %}

{% block title %}Mouvement{% endblock %}

{% block body %}
    <h1>Mouvement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mouvement.id }}</td>
            </tr>
            <tr>
                <th>Quantite</th>
                <td>{{ mouvement.quantite }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ mouvement.date ? mouvement.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Reference</th>
                <td>{{ mouvement.reference }}</td>
            </tr>
            <tr>
                <th>DebutSerie</th>
                <td>{{ mouvement.debutSerie }}</td>
            </tr>
            <tr>
                <th>FinSerie</th>
                <td>{{ mouvement.finSerie }}</td>
            </tr>
            <tr>
                <th>Observation</th>
                <td>{{ mouvement.observation }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_mouvement_index') }}">back to list</a>

    <a href="{{ path('app_mouvement_edit', {'id': mouvement.id}) }}">edit</a>

    {{ include('mouvement/_delete_form.html.twig') }}
{% endblock %}
